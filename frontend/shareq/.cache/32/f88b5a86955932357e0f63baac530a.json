:"Hooks/useQueueWS.js","original":{"line":44,"column":11}},{"name":"songs","generated":{"line":81,"column":10},"source":"Hooks/useQueueWS.js","original":{"line":44,"column":12}},{"generated":{"line":81,"column":15},"source":"Hooks/useQueueWS.js","original":{"line":44,"column":11}},{"name":"addSong","generated":{"line":81,"column":17},"source":"Hooks/useQueueWS.js","original":{"line":44,"column":19}},{"generated":{"line":81,"column":24},"source":"Hooks/useQueueWS.js","original":{"line":44,"column":11}},{"generated":{"line":81,"column":25},"source":"Hooks/useQueueWS.js","original":{"line":44,"column":4}},{"generated":{"line":82,"column":0},"source":"Hooks/useQueueWS.js","original":{"line":45,"column":1}}],"sources":{"Hooks/useQueueWS.js":"import {useState, useEffect} from 'react'\n\n\n\nexport default function useQueueWS(queue, ws){\n    const [songs, setSongs] = useState([])    \n\n    useEffect(()=>{\n        ws.onmessage = (e) => {\n            const data = JSON.parse(e.data)\n            console.log(data)\n            switch(data.command){\n              case 'fetch':\n                setSongs(data.songs)\n                break;\n              case 'new_song':\n                console.log(\"adding new song\")\n                setSongs([\n                  ...songs,\n                  {\n                    id: data.payload.id,\n                    name: data.payload.name,\n                    votes: 0\n                  }\n                ])\n                break;\n              default:\n                console.log(\"default\")\n                break;\n            }\n        }\n      \n        ws.onopen = () => {\n            ws.send(JSON.stringify({command: 'fetchSongs', payload:queue}))\n        }\n        return () => ws.close()\n    }, [])\n\n    const addSong = (item) => {\n        console.log(\"got here\")\n        ws.send(JSON.stringify({command:'new_song', payload:{queue:queue, name:item.name, uri:item.uri}}))\n    }\n\n    return [songs, addSong]\n}"},"